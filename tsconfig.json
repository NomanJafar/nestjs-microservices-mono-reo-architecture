{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/common": [
        "libs/common/src",
      ],
      "@app/common/*": [
        "libs/common/src/*",
      ],
      "@app/tt.analytics.mongo": [
        "libs/tt.analytics.mongo/src"
      ],
      "@app/tt.analytics.mongo/*": [
        "libs/tt.analytics.mongo/src/*"
      ],
      "@app/configuration": [
        "libs/configuration/src"
      ],
      "@app/configuration/*": [
        "libs/configuration/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ],
      "@app/core": [
        "libs/core/src",
      ],
      "@app/core/*": [
        "libs/core/src/*",
      ],
      "@app/users": [
        "libs/users/src"
      ],
      "@app/users/*": [
        "libs/users/src/*"
      ],
      "@app/features": [
        "libs/features/src"
      ],
      "@app/features/*": [
        "libs/features/src/*"
      ],
      "@app/signals.sql": [
        "libs/signals.sql/src"
      ],
      "@app/signals.sql/*": [
        "libs/signals.sql/src/*"
      ],
      "@app/trackings.sql": [
        "libs/trackings.sql/src"
      ],
      "@app/trackings.sql/*": [
        "libs/trackings.sql/src/*"
      ]
    }
  }
}