{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "ES2021",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@app/common": [
        "libs/common/src",
        "libs/common/src"
      ],
      "@app/common/*": [
        "libs/common/src/*",
        "libs/common/src/*"
      ],
      "@app/shared-services": [
        "libs/shared-services/src"
      ],
      "@app/shared-services/*": [
        "libs/shared-services/src/*"
      ],
      "@app/tt.core.sql": [
        "libs/tt.core.sql/src"
      ],
      "@app/tt.core.sql/*": [
        "libs/tt.core.sql/src/*"
      ],
      "@app/tt.telemetry.sql": [
        "libs/tt.telemetry.sql/src"
      ],
      "@app/tt.telemetry.sql/*": [
        "libs/tt.telemetry.sql/src/*"
      ],
      "@app/tt.analytics.mongo": [
        "libs/tt.analytics.mongo/src"
      ],
      "@app/tt.analytics.mongo/*": [
        "libs/tt.analytics.mongo/src/*"
      ],
      "@app/configuration": [
        "libs/configuration/src"
      ],
      "@app/configuration/*": [
        "libs/configuration/src/*"
      ],
      "@app/logger": [
        "libs/logger/src"
      ],
      "@app/logger/*": [
        "libs/logger/src/*"
      ]
    }
  }
}