{
  "name": "tt.performance",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "concurrently \"npm run start:api\" \"npm run start:alerts\" \"npm run start:maintenance\" \"npm run start:signals\" \"npm run start:signals_processing\"",
    "start:api": "nest start api",
    "start:alerts": "nest start alerts",
    "start:maintenance": "nest start maintenance",
    "start:signals": "nest start signals",
    "start:signals_processing": "nest start signals_processing",
    "start:dev": "concurrently \"npm run start:dev:api\" \"npm run start:dev:alerts\" \"npm run start:dev:maintenance\" \"npm run start:dev:signals\" \"npm run start:dev:signals_processing\"",
    "start:dev:api": "cross-env APP_NAME=api NODE_ENV=development nest start api --watch",
    "start:dev:alerts": "cross-env APP_NAME=alerts NODE_ENV=development nest start alerts --watch",
    "start:dev:maintenance": "cross-env APP_NAME=maintenance NODE_ENV=development nest start maintenance --watch",
    "start:dev:signals": "cross-env APP_NAME=signals NODE_ENV=development nest start signals --watch",
    "start:dev:signals_processing": "cross-env APP_NAME=signals_processing NODE_ENV=development nest start signals_processing --watch",
    "build": "nest build",
    "build:all": "concurrently \"npm run build:api\" \"npm run build:alerts\" \"npm run build:maintenance\" \"npm run build:signals\" \"npm run build:signals_processing\"",
    "build:api": "nest build api",
    "build:alerts": "nest build alerts",
    "build:maintenance": "nest build maintenance",
    "build:signals": "nest build signals",
    "build:signals_processing": "nest build signals_processing",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:debug": "concurrently \"npm run start:debug:api\" \"npm run start:debug:alerts\" \"npm run start:debug:maintenance\" \"npm run start:debug:signals\" \"npm run start:debug:signals_processing\"",
    "start:debug:api": "nest start --debug api --watch",
    "start:debug:alerts": "nest start --debug alerts --watch",
    "start:debug:maintenance": "nest start --debug maintenance --watch",
    "start:debug:signals": "nest start --debug signals --watch",
    "start:debug:signals_processing": "nest start --debug signals_processing --watch",
    "start:prod": "concurrently \"npm run start:prod:api\" \"npm run start:prod:alerts\" \"npm run start:prod:maintenance\" \"npm run start:prod:signals\" \"npm run start:prod:signals_processing\"",
    "start:prod:api": "node dist/apps/api/main",
    "start:prod:alerts": "node dist/apps/alerts/main",
    "start:prod:maintenance": "node dist/apps/maintenance/main",
    "start:prod:signals": "node dist/apps/signals/main",
    "start:prod:signals_processing": "node dist/apps/signals_processing/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:all": "concurrently \"npm run test:api\" \"npm run test:alerts\" \"npm run test:maintenance\" \"npm run test:signals\" \"npm run test:signals_processing\"",
    "test:api": "jest --config ./apps/api/jest.config.js",
    "test:alerts": "jest --config ./apps/alerts/jest.config.js",
    "test:maintenance": "jest --config ./apps/maintenance/jest.config.js",
    "test:signals": "jest --config ./apps/signals/jest.config.js",
    "test:signals_processing": "jest --config ./apps/signals_processing/jest.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/api/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "mongoose": "^8.5.2",
    "nest-winston": "^1.9.7",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "winston": "^3.13.1",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/tt.core.sql(|/.*)$": "<rootDir>/libs/core/tt.core.sql/src/$1",
      "^@app/tt.telemetry.sql(|/.*)$": "<rootDir>/libs/core/tt.telemetry.sql/src/$1",
      "^@app/tt.analytics.mongo(|/.*)$": "<rootDir>/libs/core/tt.analytics.mongo/src/$1",
      "^@app/configuration(|/.*)$": "<rootDir>/libs/core/configuration/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/core/logger/src/$1",
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1",
      "^@app/features(|/.*)$": "<rootDir>/libs/features/src/$1",
      "^@app/signals.sql(|/.*)$": "<rootDir>/libs/signals.sql/src/$1",
      "^@app/trackings.sql(|/.*)$": "<rootDir>/libs/trackings.sql/src/$1"
    }
  }
}