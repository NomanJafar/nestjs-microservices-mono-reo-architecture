{
  "name": "tt.performance",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "concurrently \"npm run start:tt.api\" \"npm run start:tt.downtime\" \"npm run start:tt.oee\" \"npm run start:tt.job_estimation\" \"npm run start:tt.telemetry\" \"npm run start:tt.telemetry_processing\"",
    "start:tt.api": "nest start tt.api",
    "start:tt.downtime": "nest start tt.downtime",
    "start:tt.oee": "nest start tt.oee",
    "start:tt.job_estimation": "nest start tt.job_estimation",
    "start:tt.telemetry": "nest start tt.telemetry",
    "start:tt.telemetry_processing": "nest start tt.telemetry_processing",
    "start:dev": "concurrently \"npm run start:dev:tt.api\" \"npm run start:dev:tt.downtime\" \"npm run start:dev:tt.oee\" \"npm run start:dev:tt.job_estimation\" \"npm run start:dev:tt.telemetry\" \"npm run start:dev:tt.telemetry_processing\"",
    "start:dev:tt.api": "nest start tt.api --watch",
    "start:dev:tt.downtime": "nest start tt.downtime --watch",
    "start:dev:tt.oee": "nest start tt.oee --watch",
    "start:dev:tt.job_estimation": "nest start tt.job_estimation --watch",
    "start:dev:tt.telemetry": "nest start tt.telemetry --watch",
    "start:dev:tt.telemetry_processing": "nest start tt.telemetry_processing --watch",
    "build": "nest build",
    "build:all": "concurrently \"npm run build:tt.api\" \"npm run build:tt.downtime\" \"npm run build:tt.oee\" \"npm run build:tt.job_estimation\" \"npm run build:tt.telemetry\" \"npm run build:tt.telemetry_processing\"",
    "build:tt.api": "nest build tt.api",
    "build:tt.downtime": "nest build tt.downtime",
    "build:tt.oee": "nest build tt.oee",
    "build:tt.job_estimation": "nest build tt.job_estimation",
    "build:tt.telemetry": "nest build tt.telemetry",
    "build:tt.telemetry_processing": "nest build tt.telemetry_processing",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start:debug": "concurrently \"npm run start:debug:tt.api\" \"npm run start:debug:tt.downtime\" \"npm run start:debug:tt.oee\" \"npm run start:debug:tt.job_estimation\" \"npm run start:debug:tt.telemetry\" \"npm run start:debug:tt.telemetry_processing\"",
    "start:debug:tt.api": "nest start --debug tt.api --watch",
    "start:debug:tt.downtime": "nest start --debug tt.downtime --watch",
    "start:debug:tt.oee": "nest start --debug tt.oee --watch",
    "start:debug:tt.job_estimation": "nest start --debug tt.job_estimation --watch",
    "start:debug:tt.telemetry": "nest start --debug tt.telemetry --watch",
    "start:debug:tt.telemetry_processing": "nest start --debug tt.telemetry_processing --watch",
    "start:prod": "concurrently \"npm run start:prod:tt.api\" \"npm run start:prod:tt.downtime\" \"npm run start:prod:tt.oee\" \"npm run start:prod:tt.job_estimation\" \"npm run start:prod:tt.telemetry\" \"npm run start:prod:tt.telemetry_processing\"",
    "start:prod:tt.api": "node dist/apps/tt.api/main",
    "start:prod:tt.downtime": "node dist/apps/tt.downtime/main",
    "start:prod:tt.oee": "node dist/apps/tt.oee/main",
    "start:prod:tt.job_estimation": "node dist/apps/tt.job_estimation/main",
    "start:prod:tt.telemetry": "node dist/apps/tt.telemetry/main",
    "start:prod:tt.telemetry_processing": "node dist/apps/tt.telemetry_processing/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:all": "concurrently \"npm run test:tt.api\" \"npm run test:tt.downtime\" \"npm run test:tt.oee\" \"npm run test:tt.job_estimation\" \"npm run test:tt.telemetry\" \"npm run test:tt.telemetry_processing\"",
    "test:tt.api": "jest --config ./apps/tt.api/jest.config.js",
    "test:tt.downtime": "jest --config ./apps/tt.downtime/jest.config.js",
    "test:tt.oee": "jest --config ./apps/tt.oee/jest.config.js",
    "test:tt.job_estimation": "jest --config ./apps/tt.job_estimation/jest.config.js",
    "test:tt.telemetry": "jest --config ./apps/tt.telemetry/jest.config.js",
    "test:tt.telemetry_processing": "jest --config ./apps/tt.telemetry_processing/jest.config.js",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/tt.api/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.0.0",
    "@nestjs/mongoose": "^10.0.10",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.5",
    "mongoose": "^8.5.2",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/common(|/.*)$": "<rootDir>/libs/common/src/$1",
      "^@app/shared-services(|/.*)$": "<rootDir>/libs/shared-services/src/$1",
      "^@app/tt.core.sql(|/.*)$": "<rootDir>/libs/tt.core.sql/src/$1",
      "^@app/tt.telemetry.sql(|/.*)$": "<rootDir>/libs/tt.telemetry.sql/src/$1",
      "^@app/tt.analytics.mongo(|/.*)$": "<rootDir>/libs/tt.analytics.mongo/src/$1",
      "^@app/configuration(|/.*)$": "<rootDir>/libs/configuration/src/$1",
      "^@app/logger(|/.*)$": "<rootDir>/libs/logger/src/$1"
    }
  }
}
